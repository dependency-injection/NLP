wiplot<-function(textname)
{
if(textname=="text1")
{
python.exec("fdist=FreqDist([len(w) for w in text1[1:10]])")
python.exec("wlen=sorted([len(w) for w in text1[pystart:pyend]])")
rfdist<-python.get("fdist") # plot this
rwlen<-python.get("wlen")
pie3D(rfdist,labels=rwlen,explode=0.1,main="Word Length Frequency ")
}
else if(textname=="text2")
{
python.exec("fdist=FreqDist([len(w) for w in text2[]])")
python.exec("wlen=[len(w) for w in text1[1:10]]")
rfdist<-python.get("fdist") # plot this
rwlen<-python.get("wlen")
pie3D(rfdist,labels=rwlen,explode=0.1,main="Word Length Frequency ")
}
else if(textname=="text2")
{
python.exec("fdist=FreqDist([len(w) for w in text1])")
python.exec("wlen=[len(w) for w in text1[1:10]]")
rfdist<-python.get("fdist") # plot this
rwlen<-python.get("wlen")
pie3D(rfdist,labels=rwlen,explode=0.1,main="Word Length Frequency ")
}
else if(textname=="text2")
{
python.exec("fdist=FreqDist([len(w) for w in text1])")
python.exec("wlen=[len(w) for w in text1[1:10]]")
rfdist<-python.get("fdist") # plot this
rwlen<-python.get("wlen")
pie3D(rfdist,labels=rwlen,explode=0.1,main="Word Length Frequency ")
}
else if(textname=="text2")
{
python.exec("fdist=FreqDist([len(w) for w in text1])")
python.exec("wlen=[len(w) for w in text1[1:10]]")
rfdist<-python.get("fdist") # plot this
rwlen<-python.get("wlen")
pie3D(rfdist,labels=rwlen,explode=0.1,main="Word Length Frequency ")
}
else if(textname=="text2")
{
python.exec("fdist=FreqDist([len(w) for w in text1])")
python.exec("wlen=[len(w) for w in text1[1:10]]")
rfdist<-python.get("fdist") # plot this
rwlen<-python.get("wlen")
pie3D(rfdist,labels=rwlen,explode=0.1,main="Word Length Frequency ")
}
else if(textname=="text2")
{
python.exec("fdist=FreqDist([len(w) for w in text1])")
python.exec("wlen=[len(w) for w in text1[1:10]]")
rfdist<-python.get("fdist") # plot this
rwlen<-python.get("wlen")
pie3D(rfdist,labels=rwlen,explode=0.1,main="Word Length Frequency ")
}
else if(textname=="text2")
{
python.exec("fdist=FreqDist([len(w) for w in text1])")
python.exec("wlen=[len(w) for w in text1[1:10]]")
rfdist<-python.get("fdist") # plot this
rwlen<-python.get("wlen")
pie3D(rfdist,labels=rwlen,explode=0.1,main="Word Length Frequency ")
}
else if(textname=="text2")
{
python.exec("fdist=FreqDist([len(w) for w in text1])")
python.exec("wlen=[len(w) for w in text1[1:10]]")
rfdist<-python.get("fdist") # plot this
rwlen<-python.get("wlen")
pie3D(rfdist,labels=rwlen,explode=0.1,main="Word Length Frequency ")
}
else
{
file<-scan(file=textname,what="list",n=-1,sep="",skip=0,na.strings="NA")
str<-paste(file,collapse=" ")
prop<-str_replace_all(str,"'","")
prop<-str_replace_all(prop,'"',"")
python.assign("pystr",prop)
python.exec("pylst=pystr.split()")
python.exec("fdist=FreqDist([len(w) for w in pylst[pystart:pyend]])")
python.exec("wlen=[len(w) for w in pylst[pystart:pyend]]")
rfdist<-python.get("fdist") # plot this
rwlen<-python.get("wlen")
pie3D(rfdist,labels=rwlen,explode=0.1,main="Word Length Frequency ")
}
}
